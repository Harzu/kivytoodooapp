#:import utils kivy.utils
#:import MDList kivymd.list.MDList
#:import OneLineListItem kivymd.list.OneLineListItem

<ItemClass@OneLineListItem>:
  text_color:utils.get_color_from_hex('#414141')
  color: utils.get_color_from_hex('#414141')

<NoteList>:
  text: ''
  canvas:
    Color:
      rgb:utils.get_color_from_hex('#414141')
  size_hint: 1, 0.7
  pos_hint: {'center_x': 0.5, 'center_y': 0.1}
